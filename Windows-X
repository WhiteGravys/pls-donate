repeat
    wait()
until game:IsLoaded()

if game.PlaceId ~= 8737602449 then
    return
end

coroutine.wrap(function()
    while wait() do
        if chat then
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/WhiteGravys/pls-donate/main/chat'),true))()
       end
     end
end)

coroutine.wrap(function()
    while wait() do
        if optimize then
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/WhiteGravys/pls-donate/main/optimize'),true))()
       end
     end
end)

loadstring(game:HttpGet(('https://raw.githubusercontent.com/WhiteGravys/pls-donate/main/main'),true))()

